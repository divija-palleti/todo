<%= form_for([@todo_list, @todo_list.todo_items.build]) do |f| %>
        <%= f.label :task %>
        <%= f.label :priority %>
        <%= f.label :duedate %> 
        <%= f.text_field :content, placeholder: "New Todo" %>
        <%= f.select :priority, TodoItem.priorities.keys %>
        <%= f.date_select :duedate, min: Date.today  %> 
        <%= f.submit %>
    
<% end %>